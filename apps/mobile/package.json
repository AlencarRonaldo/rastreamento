{
  "name": "@vehicle-tracking/mobile",
  "version": "1.0.0",
  "description": "Mobile app para rastreamento veicular com SuperClaude",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit": "eas submit",
    "update": "eas update",
    "dev": "expo start --dev-client",
    "test": "jest --config ../../jest.config.js",
    "_lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "superclaude:implement": "python3 ../../.superclaude/commands/mobile-implement.py",
    "superclaude:design": "python3 ../../.superclaude/commands/mobile-design.py"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.2.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@turf/turf": "^6.5.0",
    "date-fns": "^2.30.0",
    "expo": "~50.0.0",
    "expo-asset": "~9.0.2",
    "expo-background-fetch": "~12.0.1",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-font": "~11.10.2",
    "expo-location": "~16.5.4",
    "expo-notifications": "~0.27.6",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "~11.7.2",
    "expo-updates": "~0.24.11",
    "immer": "^10.0.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-native": "0.73.4",
    "react-native-background-job": "^2.3.1",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-map-clustering": "^3.4.2",
    "react-native-maps": "1.10.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6",
    "socket.io-client": "^4.7.4",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/webpack-config": "^19.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "detox": "^20.13.5",
    "eslint": "^8.0.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3"
  },
  
  "private": true,
  "superclaude": {
    "personas": [
      "mobile-frontend",
      "gps-architect"
    ],
    "commands": [
      "/sc:implement mobile-ui",
      "/sc:implement real-time-tracking",
      "/sc:design user-experience",
      "/sc:test mobile-app"
    ],
    "mcp_integrations": [
      "context7",
      "magic",
      "playwright"
    ]
  }
}
